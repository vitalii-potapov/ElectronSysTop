{
  "name": "sys-top",
  "productName": "SysTop",
  "version": "1.0.0",
  "description": "Monitor your system CPU",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec cross-env NODE_ENV=development electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SysTop\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Vitalii Potapov",
  "license": "ISC",
  "dependencies": {
    "electron": "^9.2.0",
    "electron-packager": "^15.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron-log": "^4.2.3",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4"
  }
}
